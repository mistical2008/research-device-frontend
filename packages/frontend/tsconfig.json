{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@app/frontend": ["../../frontend/src"],
      "@app/backend": ["../../backend/src"],
      "@app/types": ["../../types/src"],
      "@app/*": ["../../*/src"],
      "~*": [ "../*" ],
      "src/*": [ "./*" ],
      "@/*": [ "../node_modules/*" ],
      "app/*": [ "./app/*" ],
      "processes/*": [ "./processes/*" ],
      "pages/*": [ "./pages/*" ],
      "widgets/*": [ "./widgets/*" ],
      "features/*": [ "./features/*" ],
      "entities/*": [ "./entities/*" ],
      "shared/*": [ "./shared/*" ]
    }
  }
}
